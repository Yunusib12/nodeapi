<div class="row bg-primary mb-3">
  <div class="col-lg-12">
    <h2>Employee List</h2>
  </div>
</div>
<ul class="list-group">
  <% listEmployeeArray.forEach((employee)=>{ %>
  <li class="list-group-item d-flex justify-content-between align-items-center">
    <a href="/api/employee/<%= employee.id %>"><%=employee.name %> </a>
  </li>
  <% }); %>
</ul>
